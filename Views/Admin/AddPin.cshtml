@model Wanc.Models.tbl_pincode
@{
    ViewBag.Title = "AddPin";
    Layout = "~/shared/_AdminLayout1.cshtml";
}

<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
<script type="text/javascript">
    $(function () {
        $("#chkCashBack").click(function () {
            if ($(this).is(":checked")) {
                $("#txtCashBack").removeAttr("disabled");
                $("#txtCashBack").focus();
            } else {
                $("#txtCashBack").attr("disabled", "disabled");
            }
        });
    });
</script>
<section class="wrapper">

    <div class="breadcrumb-line">
        <ul class="breadcrumb">
            <li><a href="/Admin/dashboard">Dashboard</a></li>
            <li class="active"><a href="#">Add Pincode</a></li>
        </ul>
    </div>
    <div class="form-horizontal">
        <div class="row">
            <div class="col-md-8">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h6 class="panel-title" style="color:green">
                            <i class="icon-bubble4"></i>
                        </h6>
                    </div>
                    <div class="panel-body">
                        <form method="post" action="~/Admin/AddPin">
                            <label class="danger" style="color:blue;font-size:13px">@TempData["msg"]</label>
                            <div class="form-group">
                                <label class="col-md-4">Enter Pin Code:</label>
                                <div class="col-md-8">
                                    <input type="text" class="form-control" placeholder="Pin Code" name="Pincode" id="Pincode" required />
                                </div>

                            </div>
                            <div class="form-group">
                                <label class="col-md-4">Enter City:</label>
                                <div class="col-md-8">
                                    <input type="text" class="form-control" placeholder="City" name="City" required />
                                </div>


                            </div>
                            <div class="form-group">
                                <label class="col-md-4">Enter State:</label>
                                <div class="col-md-8">
                                    <input type="text" class="form-control" placeholder="State" name="State" required />
                                </div>


                            </div>
                            <div class="form-actions text-center">
                                <button type="submit" class="btn btn-primary btn-sm">Submit</button>

                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<div class="footer clearfix">
    <div class="text-center">© 2017 Pride Bazaar</div>
</div>


<script src="~/Scripts/jquery.validate-vsdoc.js"></script>
<script src="~/Scripts/jquery.validate.min.js"></script>
<script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>

<script>
    function FillScat() {
        var CategoryId = $('#categoryId').val();
        var url = "/Admin/FillSubCategory?Id=" + CategoryId;
        debugger;
        $.ajax({
            url: url,
            type: "GET",
            dataType: "JSON",
            success: function (subcatdata) {
                $("#fnSubCategory").html("");
                $("#fnSubCategory").append(subcatdata);
            }
        });
    }
</script>

<script type="text/javascript">

    function show(input) {
        if (input.files && input.files[0]) {
            var filerdr = new FileReader();
            filerdr.onload = function (e) {
                $('#user_img').attr('src', e.target.result);
            }
            filerdr.readAsDataURL(input.files[0]);
        }
    }
</script>

<script>
    function cdvalue() {
        debugger;
        var value = $("#price").val() * 1;
        var per = $("#valuecd").val() * 1;
        var tree = per * value;
        var four = tree / 100;
        var final = value - four;
        $("#dvalue").val(final);
    };
</script>

<script type="text/javascript">
    function OnlyNumeric(e) {
        if ((e.which < 48 || e.which > 57)) {
            if (e.which == 8 || e.which == 46 || e.which == 0) {
                return true;
            }
            else {
                return false;
            }
        }
    }
</script>
