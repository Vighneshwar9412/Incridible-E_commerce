@model Wanc.Models.Admin
@{
    ViewBag.Title = "Index";
}
<!DOCTYPE html>
<html>
<head>
    <title>WANC Login</title>
    <link rel="icon" href="/images/favicon.png">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="Grofers Responsive web template, Bootstrap Web Templates, Flat Web Templates, Android Compatible web template,
Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!-- bootstrap-css -->
    <link rel="stylesheet" href="/Admin_Design/css/bootstrap.min.css">
    @*<link rel="shortcut icon" type="image/ico" href="/Home_Design/images/ellegorlogo.png" />*@
    <!-- //bootstrap-css -->
    <!-- Custom CSS -->
    <link href="/Admin_Design/css/style.css" rel='stylesheet' type='text/css' />
    <link href="/Admin_Design/css/style-responsive.css" rel="stylesheet" />
    <!-- font CSS -->
    <link href='//fonts.googleapis.com/css?family=Roboto:400,100,100italic,300,300italic,400italic,500,500italic,700,700italic,900,900italic' rel='stylesheet' type='text/css'>
    <!-- font-awesome icons -->
    <link rel="stylesheet" href="/Admin_Design/css/font.css" type="text/css" />
    <link href="/Admin_Design/css/font-awesome.css" rel="stylesheet">
    <!-- //font-awesome icons -->
    <script src="/Admin_Design/js/jquery2.0.3.min.js"></script>
</head>
<body>

    <div class="log-w3">
        <div class="w3layouts-main">
            @if (ViewBag.mode != null)
            {
                <div style="text-align:center; color:darkred;">@ViewBag.mode</div><br />
            }
            <h2>Sign In Now</h2>
            <form action="/Admin/Index" method="post">
                <input type="text" class="ggg" name="logingid" placeholder="User-Name" required="">
                <input type="password" class="ggg" name="pwd" placeholder="PASSWORD" required="">
                <div class="clearfix"></div>
                <input type="submit" value="Sign In" name="login">
            </form>

        </div>

    </div>
    <script src="/Admin_Design/js/bootstrap.js"></script>
    <script src="/Admin_Design/js/jquery.dcjqaccordion.2.7.js"></script>
    <script src="/Admin_Design/js/scripts.js"></script>
    <script src="/Admin_Design/js/jquery.slimscroll.js"></script>
    <script src="/Admin_Design/js/jquery.nicescroll.js"></script>
    <!--[if lte IE 8]><script language="javascript" type="text/javascript" src="js/flot-chart/excanvas.min.js"></script><![endif]-->
    <script src="/Admin_Design/js/jquery.scrollTo.js"></script>
</body>
</html>
